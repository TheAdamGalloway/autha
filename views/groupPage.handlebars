{{#if team}}
	<h1>Team: <strong>{{team.name}}</strong></h1>
	<p>Description: <strong>{{team.description}}</strong>
	<p>Number of Keys: <strong>{{team.keyCount}}</strong>
	<p>Number of Members: <strong>{{team.memberCount}}</strong>
{{/if}}
{{#if organisation}}
	<h1>Organisation: <strong>{{organisation.name}}</strong></h1>
	<p>Description: <strong>{{organisation.description}}</strong>
	<p>Number of Groups: <strong>{{organisation.teamCount}}</strong></p>
	<p>Number of Keys: <strong>{{organisation.keyCount}}</strong>
	<p>Number of Members: <strong>{{organisation.memberCount}}
{{/if}}

{{#if keys}}
	<hr>
	<h2>Keys</h2>
	{{#each keys}}
		<div class="col-md-6">
			<div class="panel panel-default">
				<div class="panel-heading">
			    	<a href="http://{{this.URL}}"> <h3 class="panel-title">{{this.name}}</h3></a>
			  	</div>
			  	<div class="panel-body">
			  		{{this.description}}
			  		<hr>
			  		<center><a class="btn btn-default generateCode" href="/code/{{this.rid}}" role="button" name="{{this.rid}}">Generate TFA Code</a></center>
			  	</div>
			</div>
		</div>
	{{/each}}
{{/if}}